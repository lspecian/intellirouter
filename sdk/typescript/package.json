{
    "name": "intellirouter",
    "version": "0.1.0",
    "description": "TypeScript SDK for IntelliRouter",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src --ext .ts",
        "format": "prettier --write \"src/**/*.ts\"",
        "docs": "typedoc",
        "prepublishOnly": "npm run build",
        "example:chat": "ts-node examples/chat-completion.ts",
        "example:stream": "ts-node examples/streaming-chat.ts",
        "example:error": "ts-node examples/error-handling.ts"
    },
    "keywords": [
        "intellirouter",
        "ai",
        "llm",
        "sdk"
    ],
    "author": "IntelliRouter Team",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.6.0",
        "eventsource-parser": "^1.0.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.0",
        "@types/node": "^20.8.0",
        "@typescript-eslint/eslint-plugin": "^6.9.0",
        "@typescript-eslint/parser": "^6.9.0",
        "eslint": "^8.52.0",
        "jest": "^29.7.0",
        "prettier": "^3.0.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typedoc": "^0.25.3",
        "typescript": "^5.2.2"
    }
}